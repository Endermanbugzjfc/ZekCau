{
   "name": "endermanbugzjfc/zekcau",
   "type": "library",
   "require": {
      "pocketmine/pocketmine-mp": "4.0.0",
      "sof3/await-std": "dev-master",
      "muqsit/fakeplayer": "dev-master"
   },
   "require-dev": {
      "phpunit/phpunit": "^9.5",
      "phpstan/phpstan": "1.4.6",
      "phpstan/phpstan-strict-rules": "^1.0",
      "phpstan/extension-installer": "^1.0"

   },
   "repositories": [{
      "type": "package",
      "package": {
         "name": "muqsit/fakeplayer",
         "version": "master",
         "autoload": {
            "classmap": ["src/"]
         },
         "source": {
            "url": "https://github.com/Muqsit/FakePlayer",
            "type": "git",
            "reference": "master"
         }
      }
   }],
   "autoload": {
      "psr-4": {
         "Endermanbugzjfc\\ZekCau\\": "src"
      }
   },
   "autoload-dev": {
      "psr-4": {
         "Endermanbugzjfc\\ZekCau\\": "tests"
      }
   },
   "config": {
      "allow-plugins": {
         "phpstan/extension-installer": true
      }
   }
}